shot.ref.getDownloadURL().then((downloadURL) => {\n        const newImage = { id: fileName, path: downloadURL };\n        props.setImages((prevState => [...prevState, newImage]))\n      });\n    })\n  }, [props.setImages])\n\n  return (\n    <>\n      <div className=\"list-image-size\">\n        {props.images.length > 0 && (\n          props.images.map(image => <ImagePreview id={image.id} path={image.path} key={image.id} deleteImage={deleteImage} />)\n        )}\n      </div>\n      <div className=\"text-right\">\n        <IconButton className={classes.icon}>\n          <label>\n            <AddAPhotoIcon />\n            <input\n              className=\"display-none\"\n              type=\"file\"\n              id=\"image\"\n              onChange={(event) => uploadImage(event)}\n            />\n          </label>\n        </IconButton>\n      </div>\n    </>\n  )\n}\n\nexport default ImageArea","/Users/kondoukento/projects/feel_memory/src/components/Article/ImegePreview.jsx",[],"/Users/kondoukento/projects/feel_memory/src/components/Article/ArticleCard.jsx",["165","166"],"/Users/kondoukento/projects/feel_memory/src/components/Header/Header.jsx",["167","168","169","170","171","172","173"],"/Users/kondoukento/projects/feel_memory/src/components/Header/SearchTag.jsx",["174","175","176"],"/Users/kondoukento/projects/feel_memory/src/reducks/feeles/operation.js",["177"],"import { push } from \"connected-react-router\";\nimport { db } from \"../../firebase\"\nimport { deleteFeelAction, fetchFeelAction } from \"./action\";\n\nconst feelesRef = db.collection(\"feeles\")\n// タグの保存\nexport const newFeel = (feel) => {\n    return async (dispatch) => {\n\n        const data = {\n            feel: feel,\n        }\n        const ref = feelesRef.doc();\n        const id = ref.id;\n        data.id = id\n\n        return feelesRef.doc(id).set(data,{marge:true})\n            .then(() => {\n                dispatch(fetchFeel())\n            }).catch((error) => {\n            throw n