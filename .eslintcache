assName={classes.icon}>\n                    <label>\n                        <AddAPhotoIcon />\n                        <input className=\"display-none\" type=\"file\" id=\"image\"\n                        onChange={(event)=>uploadImage(event)} />\n                    </label>\n                </IconButton>\n            </div>\n        </>\n    )\n}\n\nexport default ImageArea","/Users/kondoukento/projects/feel_memory/src/components/Image/ImegePreview.jsx",["135"],"import React from 'react'\n\n\nconst ImagePreview=(props)=>{\n    return(\n        <div className=\"image-size\" onClick={()=>props.deleteImage(props.id)}>\n            <img src={props.path} />\n        </div>\n    )\n}\n\nexport default ImagePreview","/Users/kondoukento/projects/feel_memory/src/reducks/article/selector.js",[],"/Users/kondoukento/projects/feel_memory/src/reducks/article/action.js",[],{"ruleId":"136","replacedBy":"137"},{"ruleId":"138","replacedBy":"139"},{"ruleId":"140","severity":1,"message":"141","line":7,"column":10,"nodeType":"142","messageId":"143","endLine":7,"endColumn":22},{"ruleId":"140","severity":1,"message":"144","line":13,"column":11,"nodeType":"142","messageId":"143","endLine":13,"endColumn":14},{"ruleId":"140","severity":1,"message":"145","line":14,"column":11,"nodeType":"142","messageId":"143","endLine":14,"endColumn":19},{"ruleId":"140","severity":1,"message":"146","line":4,"column":8,"nodeType":"142","messageId":"143","endLine":4,"endColumn":14},{"ruleId":"147","severity":1,"message":"148","line":16,"column":8,"nodeType":"149","endLine":16,"endColumn":10,"suggestions":"150"},{"ruleId":"140","severity":1,"message":"151","line":5,"column":10,"nodeType":"142","messageId":"143","endLine":5,"endColumn":23},{"ruleId":"147","severity":1,"message":"152","line":38,"column":7,"nodeType":"149","endLine":38,"endColumn":24,"suggestions":"153"},{"ruleId":"147","severity":1,"message":"152","line":51,"column":7,"nodeType":"149","endLine":51,"endColumn":21,"suggestions":"154"},{"ruleId"